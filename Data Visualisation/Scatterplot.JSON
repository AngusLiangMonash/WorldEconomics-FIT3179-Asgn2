{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/AngusLiangMonash/WorldEconomics-FIT3179-Asgn2/main/WorldEco_data/corruption.csv"},
    "width": 500,
    "height": 400,
    "padding": 5,
    "title": {
      "text":"Corruption Index and Annual Income", 
      "fontSize": 20,
      "font": "Dosis"
      },
      
     "mark": {
      "type": "circle",
      "size": 50,
      "tooltip": {"content": "data"}},
      "autosize": "fit-x",
    "encoding": {
      "x": {
        "field": "corruption_index", 
        "type": "quantitative",
        "title": "Corruption Index"
        },
      "y": {
        "field": "annual_income", 
        "type": "quantitative",
        "scale": {"zero": false},
        "title": "Annual Income (USD)"
        
        }
      }
      
    }
  